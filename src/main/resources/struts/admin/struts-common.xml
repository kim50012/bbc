<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="admin/ueditor" extends="portal-default" namespace="/admin/ueditor">
		<action name="*" method="{1}" class="ueditorAction">
		    <result name="{1}">/admin/js/ueditor/jsp/controller.jsp</result>
		</action>
	</package> 
	<package name="admin/uploader" extends="portal-default" namespace="/admin/uploader">
		<action name="*" method="{1}" class="uploadAction">
		   <result name="{1}">/admin/uploader/{1}.jsp</result>
		</action>
	</package> 
	 
</struts>

