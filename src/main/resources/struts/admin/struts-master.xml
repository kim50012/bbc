<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<package name="admin/master/code" extends="portal-default" namespace="/admin/master/code">
		<action name="*" method="{1}" class="adminCodeAction">
			<result name="{1}">/admin/master/code/{1}.jsp</result>
		</action>
	</package>

	<package name="admin/master/user" extends="portal-default" namespace="/admin/master/user">
		<action name="*" method="{1}" class="adminMasterUserAction">
			<result name="{1}">/admin/master/user/{1}.jsp</result>
		</action>
	</package>
	<package name="admin/master/file" extends="portal-default" namespace="/admin/master/file">
		<action name="*" method="{1}" class="adminFileAction">
			<result name="{1}">/admin/master/file/{1}.jsp</result>
		</action>
	</package>
	<package name="admin/master/menu" extends="portal-default" namespace="/admin/master/menu">
		<action name="*" method="{1}" class="adminMenuAction">
			<result name="{1}">/admin/master/menu/{1}.jsp</result>
		</action>
	</package>

	<package name="admin/master/i18n" extends="portal-default" namespace="/admin/master/i18n">
		<action name="*" method="{1}" class="adminI18nAction">
			<result name="{1}">/admin/master/i18n/{1}.jsp</result>
		</action>
	</package>

	<package name="admin/master/page" extends="portal-default" namespace="/admin/master/page">
		<action name="*" method="{1}" class="adminPageMasterAction">
			<result name="{1}">/admin/master/page/{1}.jsp</result>
		</action>
	</package>

</struts>