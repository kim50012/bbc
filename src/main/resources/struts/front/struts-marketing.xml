<struts>
	
 	<package name="marketing/webzine" extends="portal-default" namespace="/marketing/webzine">
		<action name="*" method="{1}" class="webzineAction">
		    <result name="webPage">/front/webzine/webPage.jsp</result>
		    <result name="webPage2">/front/webzine/webPage2.jsp</result>
		    <result name="webPage3">/front/webzine/webPage3.jsp</result>
		    <result name="webPage4">/front/webzine/webPage4.jsp</result>
		    <result name="webPage5">/front/webzine/webPage5.jsp</result>
		    <result name="{1}">/front/webzine/{1}.jsp</result>
		</action>
	</package>
	
</struts>