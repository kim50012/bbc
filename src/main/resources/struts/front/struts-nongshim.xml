<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="front/nongshim" extends="portal-default" namespace="/front/nongshim">
		<action name="*" method="{1}" class="entryFormAction">
		    <result name="entryForm">/front/nongshim/entryForm.jsp</result>
		</action>
	</package>
  
	<package name="front/nongshim/newsfeed" extends="portal-default" namespace="/front/nongshim/newsfeed">
	    <action name="*" method="{1}" class="newsFeedNongshimAction">
