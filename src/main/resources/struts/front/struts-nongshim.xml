		</action>
	</package>
	<package name="front/nongshim/index" extends="portal-default" namespace="/front/nongshim/index">
		<action name="*" method="{1}" class="indexNongshimAction">
		    <result name="indexNongshim">/front/nongshim/{1}.jsp</result>
		</action>
	</package> 
	<package name="front/nongshim/month" extends="portal-default" namespace="/front/nongshim/month">
		<action name="*" method="{1}" class="monthLaterAction">
		    <result name="{1}">/front/nongshim/{1}.jsp</result>
		</action>
	</package> 
	<package name="front/nongshim/bSSStory" extends="portal-default" namespace="/front/nongshim/bSSStory">
		<action name="*" method="{1}" class="bSSStoryAction">
		    <result name="{1}">/front/nongshim/{1}.jsp</result>
		</action>
	</package> 
	<package name="front/nongshim/expert" extends="portal-default" namespace="/front/nongshim/expert">
