		    <result name="{1}">/front/nongshim/{1}.jsp</result>
		</action>
	</package> 
	<package name="front/nongshim/fiveFeel" extends="portal-default" namespace="/front/nongshim/fiveFeel">
		<action name="*" method="{1}" class="fiveFeelAction">
		    <result name="{1}">/front/nongshim/{1}.jsp</result>
		</action>
	</package> 
	<package name="front/nongshim/introduce" extends="portal-default" namespace="/front/nongshim/introduce">
		<action name="*" method="{1}" class="introduceAction">
		    <result name="{1}">/front/nongshim/{1}.jsp</result>
		</action>
	</package> 
	<package name="front/nongshim/publish" extends="portal-default" namespace="/front/nongshim/publish">
		<action name="*" method="{1}" class="publishAction">
		    <result name="{1}">/front/nongshim/{1}.jsp</result>
		</action>
	</package> 
</struts>