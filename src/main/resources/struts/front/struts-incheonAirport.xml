		    <result name="{1}">/front/incheonAirport/{1}.jsp</result>
		</action>
	</package> 
	  
	<package name="front/incheonAirport/transferInfo" extends="portal-default" namespace="/front/incheonAirport/transferInfo">
		<action name="*" method="{1}" class="transferInfoAction"> 
		    <result name="{1}">/front/incheonAirport/{1}.jsp</result>
		</action>
	</package> 
	  
</struts>