<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="File">
	 
	 
	  <select id="selectImgPathById" parameterClass="java.util.HashMap" resultClass="java.util.HashMap" remapResults="true">
        <![CDATA[
        	EXEC [SELECT_IMG_PATH_BY_ID]
        			@TYPE = #type#
        			,@ID = #id#
       	]]>
    </select>
</sqlMap>